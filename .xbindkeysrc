"simpleswitcher -now"
  Mod4 + Tab
"slock"
  Control+Alt+Shift+l
"xkill"
  Control+Alt+Shift+k
"cd /tmp && scrot && sleep .5 && notify-send 'Screenshot saved to /tmp'"
  Control+Alt+Shift+a

# audio keys..
"banshee --toggle-playing"
  XF86AudioPlay
"banshee --next"
  XF86AudioNext
"banshee --previous"
  XF86AudioPrev
"banshee --stop"
  XF86AudioStop

# Logitech Mouse..
"notify-send www"
  XF86WWW
"notify-send launch1"
  XF86Launch1
"notify-send comb"
  release + mod4 + XF86WWW
